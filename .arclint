{
  "exclude": [
    "(^legacy/)",
    "(\\.project$)",
    "(\\.phutil_module_cache$)"
  ],
  "linters": {
    "chmod": {
      "type": "chmod"
    },
    "filename": {
      "type": "filename"
    },
    "json": {
      "type": "json",
      "include": [
        "(^examples/.*\\.arcconfig\\.example$)",
        "(^examples/.*\\.arclint\\.example$)",
        "(^\\.arcconfig$)",
        "(^\\.arclint$)",
        "(\\.json$)"
      ]
    },
    "generated": {
      "type": "generated"
    },
    "nolint": {
      "type": "nolint"
    },
    "php": {
      "type": "php",
      "include": "(\\.php$)"
    },
    "phpcs": {
      "type": "phpcs",
      "flags": [
        ""
      ],
      "include": "(\\.php)",
      "severity.rules": {
        "(PHPCS.E.PEAR.WhiteSpace.ScopeIndent.Incorrect)": "disabled",
        "(PHPCS.W.Generic.Files.LineLength.TooLong)": "disabled",
        "(PHPCS.E.PEAR.Commenting.FileComment.Missing)": "disabled",
        "(PHPCS.E.PEAR.Commenting.ClassComment.Missing)": "disabled",
        "(PHPCS.E.PEAR.Commenting.FunctionComment.Missing)": "disabled",
        "(PHPCS.E.PEAR.NamingConventions.ValidFunctionName.PrivateNoUnderscore)": "disabled",
        "(^PHPCS\\.E\\.)": "warning",
        "(^PHPCS\\.W\\.)": "advice"
      }
    },
    "spelling": {
      "type": "spelling"
    },
    "text": {
      "type": "text",
      "severity": {
        "3": "disabled"
      }
    }
  }
}
